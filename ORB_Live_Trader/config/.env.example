# Alpaca API Credentials (for news + market data)
# Get these from: https://alpaca.markets/
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here

# TradeZero Credentials (for order execution)
# Your TradeZero login credentials
TRADEZERO_USERNAME=your_username_here
TRADEZERO_PASSWORD=your_password_here

# Optional: Set to "paper" for paper trading or "live" for real money
TRADING_MODE=paper
